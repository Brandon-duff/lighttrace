# CLIENT GRAPH

scalar Any

type Session {
  id: ID!
  user_id: ID!
  organization_id: ID!
  details: String
}

type Mutation {
  initializeSession(organization_id: ID!, details: String!): Session
  identifySession(
    session_id: ID!
    user_identifier: String!
    user_object: Any
  ): ID
  addProperties(session_id: ID!, properties_object: Any): ID
  pushPayload(session_id: ID!, events: String!, messages: String!): ID
}
